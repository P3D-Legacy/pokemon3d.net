{
    "name": "p3d-legacy/pokemon3d.net",
    "type": "project",
    "description": "",
    "keywords": [
        "website",
        "skin",
        "gamejolt",
        "pokemon3d",
        "laravel"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dsbilling/laravel-language"
        }
    ],
    "license": "gpl-3.0-only",
    "require": {
        "php": "^8.2",
        "ext-gd": "*",
        "akaunting/laravel-language": "dev-fix-agent-dependency",
        "alibayat/laravel-categorizable": "dev-master",
        "alibayat/laravel-commentable": "dev-master",
        "anlutro/l4-settings": "^1.4",
        "assada/laravel-achievements": "^2.3",
        "betterapp/laravel-db-encrypter": "^1.0",
        "beyondcode/laravel-favicon": "^1.6.0",
        "blade-ui-kit/blade-ui-kit": "^0.6",
        "cebe/php-openapi": "^1.7",
        "cyrildewit/eloquent-viewable": "^7.0",
        "dedoc/scramble": "^0.9",
        "digikraaft/laravel-review-rating": "^3.1",
        "doctrine/dbal": "^3.5",
        "filament/filament": "^3.2",
        "gabrielelana/byte-units": "^0.5.0",
        "glorand/laravel-model-settings": "^7.0",
        "graham-campbell/markdown": "^15.2",
        "guzzlehttp/guzzle": "^7.2",
        "guzzlehttp/psr7": "^2.1",
        "harrk/gamejolt-api": "^1.1",
        "intervention/image": "^2.7",
        "kilobyteno/laravel-plausible": "^1.2.0",
        "kkomelin/laravel-translatable-string-exporter": "^1.22",
        "knuckleswtf/scribe": "^4.33",
        "laravel/framework": "^11.7",
        "laravel/jetstream": "^5.0",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.11",
        "laravel/tinker": "^2.9",
        "league/html-to-markdown": "^5.1",
        "livewire/livewire": "^3.3",
        "martian/spammailchecker": "dev-main",
        "martinbean/socialite-discord-provider": "^1.2",
        "michael-rubel/laravel-formatters": "^8.0",
        "mtownsend/read-time": "^2.0",
        "ohdearapp/ohdear-php-sdk": "^3.5",
        "origami/consent": "^2.4.0",
        "overtrue/laravel-like": "^5.3",
        "propaganistas/laravel-disposable-email": "^2.3",
        "pxlrbt/filament-activity-log": "^1.1",
        "restcord/restcord": "dev-develop",
        "sentry/sentry-laravel": "^4.10",
        "socialiteproviders/facebook": "^4.1",
        "socialiteproviders/twitch": "^5.3",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-medialibrary": "^11.10",
        "spatie/laravel-permission": "^6.10",
        "spatie/laravel-stats": "^2.3",
        "spatie/laravel-tags": "^4.6",
        "symfony/http-client": "^7.0",
        "symfony/postmark-mailer": "^7.0",
        "torann/geoip": "^3.0",
        "touhidurabir/laravel-multi-key-route-binding": "^1.0",
        "tumblr/tumblr": "^0.4.1",
        "wire-elements/modal": "^2.0",
        "wire-elements/spotlight": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "barryvdh/laravel-ide-helper": "^3.2",
        "fakerphp/faker": "^1.24",
        "laravel-lang/lang": "^13.0",
        "laravel-lang/publisher": "^16.4",
        "laravel/pint": "^1.21",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^2.23",
        "pestphp/pest-plugin-laravel": "^2.2",
        "spatie/laravel-ignition": "^2.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "treeware/plant": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    }
}
